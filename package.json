  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && powershell -Command \"Copy-Item -Path public\\youtube_videos.json -Destination build\\youtube_videos.json -Force; Copy-Item -Path public\\thumbnails -Destination build\\thumbnails -Recurse -Force; Copy-Item -Path public\\data -Destination build\\data -Recurse -Force\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "A web-based tool for comparing heat map videos side by side",
  "dependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "autoprefixer": "^10.4.21",
    "framer-motion": "^12.5.0",
    "googleapis": "^146.0.0",
    "postcss": "^8.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^4.9.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "gh-pages": "^5.0.0"
  },
